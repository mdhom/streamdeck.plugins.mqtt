{
  "$schema": "https://raw.githubusercontent.com/GeekyEggo/SharpDeck/master/src/SharpDeck.PropertyInspector/manifest-schema.json",
  "Name": "Servus",
  "Version": "1.0.0",
  "Author": "Max Dhom & Christian Dirnhofer",
  "Actions": [
    {
      "Name": "Publisher",
      "UUID": "org.mdwd.streamdeck.plugins.mqtt.publisher",
      "Icon": "Images/Counter/Icon",
      "Tooltip": "Publish something",
      "States": [
        {
          "Image": "Images/Counter/Image"
        }
      ]
    },
    {
      "Name": "Subscriber",
      "UUID": "org.mdwd.streamdeck.plugins.mqtt.subscriber",
      "Icon": "Images/Reset/Icon",
      "Tooltip": "Subscribe and display",
      "States": [
        {
          "Image": "Images/Reset/Image"
        }
      ]
    }
  ],
  "Category": "Servus",
  "CategoryIcon": "Images/Plugin/CategoryIcon",
  "CodePath": "streamdeck.plugins.mqtt.exe",
  "Description": "StreamDeck plugins von servus.software",
  "PropertyInspectorPath": "PropertyInspector/property_inspector.html",
  "Icon": "Images/Plugin/Icon",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "4.1"
  },
  "OS": [
    {
      "MinimumVersion": "10",
      "Platform": "windows"
    }
  ]
}
